<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dbscout.controller.Login">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="button" layoutX="296.0" layoutY="249.0" mnemonicParsing="false" onAction="#logIn" text="Login" />
            <TextField fx:id="userId" layoutX="240.0" layoutY="188.0" promptText="user ID" />
            <Label fx:id="errorID" layoutX="305.0" layoutY="221.0" textFill="#f50a0a" />
      </children>
    </AnchorPane>
  </children>
</VBox>
